  <name>Build</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_3" name="Maven deploy" type="Maven2">
        <conditions>
          <equals name="teamcity.build.branch.is_default" value="true" />
